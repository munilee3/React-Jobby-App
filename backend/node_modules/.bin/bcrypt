#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/nxtwave/React-Jobby-App/backend/node_modules/.pnpm/bcryptjs@3.0.2/node_modules/bcryptjs/bin/node_modules:/home/nxtwave/React-Jobby-App/backend/node_modules/.pnpm/bcryptjs@3.0.2/node_modules/bcryptjs/node_modules:/home/nxtwave/React-Jobby-App/backend/node_modules/.pnpm/bcryptjs@3.0.2/node_modules:/home/nxtwave/React-Jobby-App/backend/node_modules/.pnpm/node_modules:/home/nxtwave/React-Jobby-App/backend/node_modules:/home/nxtwave/React-Jobby-App/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/React-Jobby-App/backend/node_modules/bcryptjs/bin/node_modules:/home/nxtwave/React-Jobby-App/backend/node_modules/bcryptjs/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/nxtwave/React-Jobby-App/backend/node_modules/.pnpm/bcryptjs@3.0.2/node_modules/bcryptjs/bin/node_modules:/home/nxtwave/React-Jobby-App/backend/node_modules/.pnpm/bcryptjs@3.0.2/node_modules/bcryptjs/node_modules:/home/nxtwave/React-Jobby-App/backend/node_modules/.pnpm/bcryptjs@3.0.2/node_modules:/home/nxtwave/React-Jobby-App/backend/node_modules/.pnpm/node_modules:/home/nxtwave/React-Jobby-App/backend/node_modules:/home/nxtwave/React-Jobby-App/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/React-Jobby-App/backend/node_modules/bcryptjs/bin/node_modules:/home/nxtwave/React-Jobby-App/backend/node_modules/bcryptjs/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../bcryptjs/bin/bcrypt" "$@"
else
  exec node  "$basedir/../bcryptjs/bin/bcrypt" "$@"
fi
